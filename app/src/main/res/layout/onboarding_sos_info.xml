<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".presentation.features.onboarding.OnboardingSosInfoFragment">

    <TextView
        android:id="@+id/tv_sos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="@string/send_sos_text"
        android:layout_marginTop="@dimen/_30sdp"
        android:textSize="@dimen/_34ssp"
        android:fontFamily="@font/dongle"
        android:textStyle="bold"/>
    <TextView
        android:id="@+id/sos_info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/tv_sos"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="@string/send_sos_info_content"
        android:layout_margin="@dimen/_30sdp"
        android:gravity="center"
        android:fontFamily="@font/dongle"
        android:textSize="@dimen/_28ssp"/>
</androidx.constraintlayout.widget.ConstraintLayout>